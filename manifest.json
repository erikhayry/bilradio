{
	"manifest_version": 2,
	"name": "Nyhetspaus",
	"version": "0.0.17",
	"description": "Tag en paus och lyssna på Dagens Eko från Sveriges Radio",
	"icons": {
		"48": "icons/on.png",
		"96": "icons/on@2x.png"
	},
	"background": {
		"scripts": [
			"dist/background.js"
		]
	},
	"options_ui": {
		"page": "player.html",
		"open_in_tab": true
	},
	"browser_action": {},
	"permissions": [
		"notifications",
		"idle",
		"https://*.sr.se/*"
	]
}
